                    { name = "<:emoji_2:1402577600060325910> **Username**", value = "```" .. (player.Name or "Unknown") .. "```", inline = true },
                    { name = "<:emoji_7:1402587793909223530> **Account Age**", value = "```" .. tostring(player.AccountAge) .. " Days```", inline = true },
